import requests

# Replace with your DefectDojo details
BASE_URL = "https://demo.defectdojo.org"
ATTACHMENT_PATH = "/media/engagements/attachments/yourfile.html"  # update with real path
FULL_URL = BASE_URL + ATTACHMENT_PATH

# If authentication is needed, add headers (example for API key)
HEADERS = {
    "Authorization": "Token YOUR_API_KEY_HERE"  # remove if not required
}

response = requests.get(FULL_URL, headers=HEADERS, stream=True)

print("Status:", response.status_code)
print("\n--- Headers ---")
for k, v in response.headers.items():
    print(f"{k}: {v}")